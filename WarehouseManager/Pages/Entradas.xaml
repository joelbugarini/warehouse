<UserControl x:Class="WarehouseManager.Pages.Entradas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True">

        <mui:ModernTab Name="tabMaster" SelectedSource="/Content/ListFactura.xaml" Layout="Tab">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Entradas" Source="/Content/ListFactura.xaml" />
                <mui:Link DisplayName="Nuevo" Source="/Content/SingleFactura.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>



    </Grid>
</UserControl>
